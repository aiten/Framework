<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Label="Versioning">
		<Authors>Herbert Aitenbichler</Authors>
		<Company>Herbert Aitenbichler</Company>
		<VersionPrefix>1.9.0</VersionPrefix>
		<Copyright>Herbert Aitenbichler</Copyright>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<PropertyGroup Label="Custom Variables">
		<SolutionNamespace>Framework</SolutionNamespace>

		<TargetFrameworkForProject>net10.0</TargetFrameworkForProject>
		<TargetFrameworkForApp>net10.0</TargetFrameworkForApp>
		<TargetFrameworkForUnitTest>net10.0</TargetFrameworkForUnitTest>
		<TargetFrameworkForWpf>net10.0-windows</TargetFrameworkForWpf>

	</PropertyGroup>

	<PropertyGroup Label="Compiler Settings">
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<AssemblyName>$(SolutionNamespace).$(MSBuildProjectName)</AssemblyName>
		<RootNamespace>$(SolutionNamespace).$(MSBuildProjectName)</RootNamespace>
	</PropertyGroup>

</Project>